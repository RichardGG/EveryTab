{
    "name": "Quick Tabs",
    "description": "Mission Control (Expose) / Task View - like tab manager",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "tabs",
        "<all_urls>"
    ],


    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },

    "browser_action": {
        "default_title": "Quick Tabs"
    }

}